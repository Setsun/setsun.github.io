,KAAK;AAAA,eAAKA,KAAK,CAACC,OAAN,GAAgB,CAAhB,GAAoB,CAAzB;AAAA,OAVpB;AAAA,oBAWY,UAAAD,KAAK;AAAA,eAAKA,KAAK,CAACC,OAAN,GAAgB,GAAhB,GAAsB,CAA3B;AAAA,OAXjB;AAAA;AAAA,IAAT;;AAoBA,MAAMG,MAAM,GAAG,SAATA,MAAS,CAACJ,KAAD;AAAA,WACb,gCAAC,yBAAD;AAAS,MAAA,MAAM,EAAEA,KAAK,CAACK,MAAvB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACE,gCAAC,KAAD,oBAAWL,KAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,QACE,gCAAC,GAAD;AACE,MAAA,OAAO,EAAEA,KAAK,CAACC,OADjB;AAEE,MAAA,cAAc,EAAE;AAAA,eAAMD,KAAK,CAACM,QAAN,EAAN;AAAA,OAFlB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MADF,EAKE,gCAAC,GAAD;AAAK,MAAA,OAAO,EAAEN,KAAK,CAACC,OAApB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MALF,EAME,gCAAC,KAAD;AAAO,MAAA,OAAO,EAAED,KAAK,CAACC,OAAtB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MANF,CADF,CADa;AAAA,GAAf;;AAaAG,EAAAA,MAAM,CAACG,YAAP,GAAsB;AACpBN,IAAAA,OAAO,EAAE,IADW;AAEpBH,IAAAA,IAAI,EAAE,GAFc;AAGpBQ,IAAAA,QAAQ,EAAE,oBAAM,CAAE;AAHE,GAAtB;iBAMeF,M;;;;;;;;;;;;4BApFTP,K;4BASAE,G;4BAiBAG,K;4BAmBAC,G;4BAoBAC,M","sourcesContent":["import React from 'react';\nimport { styled } from 'linaria/react';\nimport { Flipped } from 'react-flip-toolkit';\n\ntype Props = {\n  flipId?: string;\n  size?: number;\n  animate?: boolean;\n  style?: Object;\n  onFinish?: Function;\n  onAnimationEnd?: Function;\n};\n\nconst World = styled.div<Props>`\n  z-index: 0;\n  height: ${({ size }) => `${size}`}px;\n  width: ${({ size }) => `${size}`}px;\n  border-radius: 50%;\n  overflow: hidden;\n  position: relative;\n`;\n\nconst Sky = styled.div<Props>`\n  background: radial-gradient(#f6c60c, #be4405);\n  height: 65%;\n  width: 100%;\n  background-size: 100% 200%;\n  background-position: ${props => (props.animate ? '0 10%' : '0 60%')};\n  animation: animateSky ease-in-out;\n  animation-duration: ${props => (props.animate ? 2 : 0)}s;\n  animation-delay: ${props => (props.animate ? 0.3 : 0)}s;\n  animation-fill-mode: forwards;\n\n  @keyframes animateSky {\n    from { background-position: 0 60%; }\n    to { background-position: 0 10%; }\n  }\n`;\n\nconst Ocean = styled.div<Props>`\n  z-index: 1;\n  position: relative;\n  background: linear-gradient(#10eac8, #006994);\n  height: 35%;\n  width: 100%;\n  background-size: 200% 200%;\n  background-position: ${props => (props.animate ? '0 0' : '0 50%')};\n  animation: animateOcean ease-in-out;\n  animation-duration: ${props => (props.animate ? 2 : 0)}s;\n  animation-delay: ${props => (props.animate ? 0.3 : 0)}s;\n  animation-fill-mode: forwards;\n\n  @keyframes animateOcean {\n    from { background-position: 0 50%; }\n    to { background-position: 0 0; }\n  }\n`;\n\nconst Sun = styled.div<Props>`\n  position: absolute;\n  left: calc(50% - 10%); /* center on x-axis */\n  top: ${props => (props.animate ? '5%' : '55%')};\n  width: 20%;\n  height: 20%;\n  border-radius: 50%;\n  background: yellow;\n  box-shadow: 0 0 20px rgba(242, 239, 136, 0.7);\n  animation: animateSun ease-in-out;\n  animation-duration: ${props => (props.animate ? 2 : 0)}s;\n  animation-delay: ${props => (props.animate ? 0.3 : 0)}s;\n  animation-fill-mode: forwards;\n\n  @keyframes animateSun {\n    from { top: 5%; }\n    to { top: 55%; }\n  }\n`;\n\nconst Sunset = (props: Props) => (\n  <Flipped flipId={props.flipId}>\n    <World {...props}>\n      <Sun\n        animate={props.animate}\n        onAnimationEnd={() => props.onFinish()}\n      />\n      <Sky animate={props.animate} />\n      <Ocean animate={props.animate} />\n    </World>\n  </Flipped>\n);\n\nSunset.defaultProps = {\n  animate: true,\n  size: 320,\n  onFinish: () => {},\n};\n\nexport default Sunset;\n"]}]}