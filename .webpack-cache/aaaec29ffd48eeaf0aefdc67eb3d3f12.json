} from 'react-flip-toolkit';\nimport { styled } from 'linaria/react';\nimport { css } from 'linaria';\n\ntype Props = {\n  children: React.ReactNode;\n  fullscreen?: boolean;\n  flipId?: string;\n}\n\nconst fullscreenCss = css`\n  z-index: 999;\n  position: fixed;\n  top: 7.5%;\n  bottom: 7.5%;\n  left: 7.5%;\n  right: 7.5%;\n  box-shadow: 0 2px 16px rgba(0, 0, 0, 0.25);\n  border-radius: 6px;\n`;\n\nconst BaseCard = styled.div`\n  margin: 0.25rem;\n  background: white;\n  color: black;\n`;\n\nconst CardContent = styled.div`\n  height: 100%;\n  padding: 1rem;\n  cursor: ${(props: Props) => !props.fullscreen && 'pointer'};\n  width: ${(props: Props) => !props.fullscreen && '300px'};\n`;\n\nconst Card = ({\n  children,\n  fullscreen,\n  flipId,\n}: Props) => (\n  <Flipped flipId={flipId}>\n    <BaseCard className={fullscreen && fullscreenCss}>\n      <Flipped inverseFlipId={flipId} scale>\n        <CardContent fullscreen={fullscreen}>\n          {children}\n        </CardContent>\n      </Flipped>\n    </BaseCard>\n  </Flipped>\n);\n\nexport default Card;\n"]}]}